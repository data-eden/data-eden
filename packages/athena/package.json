{
  "name": "@data-eden/athena",
  "version": "0.6.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/data-eden/data-eden.git",
    "directory": "packages/athena"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
