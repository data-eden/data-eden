{
  "name": "@data-eden/codegen",
  "version": "0.12.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/data-eden/data-eden.git",
    "directory": "packages/athena"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./babel-plugin": {
      "types": "./dist/babel-plugin.d.ts",
      "import": "./dist/babel-plugin.js",
      "require": "./dist/babel-plugin.cjs"
    },
    "./gql": {
      "types": "./dist/gql.d.ts",
      "import": "./dist/gql.js",
      "require": "./dist/gql.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "athena-codegen": "./bin/cli.js"
  },
  "files": [
    "package.json",
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "dev": "tsc --watch",
    "test": "vitest run"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@babel/helper-plugin-utils": "^7.21.5",
    "@babel/types": "^7.21.5",
    "@graphql-codegen/core": "^3.1.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/plugin-helpers": "^4.2.0",
    "@graphql-codegen/typed-document-node": "^4.0.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@graphql-tools/documents": "^0.1.0",
    "@swc/core": "^1.3.58",
    "@types/debug": "^4.1.7",
    "babel-plugin-graphql-tag": "^3.3.0",
    "babel-plugin-jsx": "^1.2.0",
    "commander": "^10.0.1",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "globby": "^13.1.4"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/babel__core": "^7.20.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/babel__template": "^7.4.1",
    "@types/babel__traverse": "^7.18.5",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.1",
    "fixturify-project": "^5.2.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.24.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.5.3",
    "typescript": "^4.9.4",
    "vitest": "^0.32.0"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  },
  "volta": {
    "node": "16.20.0",
    "extends": "../../package.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
